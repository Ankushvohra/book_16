@model book.models.Campany
<div class="alert text-center" role="alert">
    Company Details
</div>
<div class="border p-2 m-2">
    <table id="tblData" class="table table-bordered table-striped table-active">
        <thead>
            <tr class="bg-primary">
                <th>Name</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>PhoneNumber</th>
                <th>st/ph</th>
                <th>IsAuthorized</th>
                <th>Action</th>
            </tr>
        </thead>
    </table>
</div>

<div class="text-center">
    <a asp-action="Upsert" class="btn btn-outline-primary"><i class="fas fa-plus">&nbsp;Add</i></a>
</div>
@section Scripts
    {
    <script src="~/js/Campany.js"></script>
}

